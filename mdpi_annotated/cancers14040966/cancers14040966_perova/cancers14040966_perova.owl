<cancers14040966_perova.owl#reviewer_12> <cancers14040966_perova.owl#number> "2"^^<http://www.w3.org/2001/XMLSchema#string> .
<cancers14040966_perova.owl#reviewer_13> <cancers14040966_perova.owl#text> "What means Pathology 0 in table 4?"^^<http://www.w3.org/2001/XMLSchema#string> .
<cancers14040966_perova.owl#reviewer_13> <cancers14040966_perova.owl#round> "1"^^<http://www.w3.org/2001/XMLSchema#string> .
